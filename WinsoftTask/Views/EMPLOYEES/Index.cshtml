@model  System.Data.DataTable

@{
    ViewBag.Title = "Index";
}

<table class="table table-bordered table-striped">

    <tr>
        <th>Employee Name</th>
        <th> Last Name</th>
        <th> HireDate</th>
        <th> Certificate</th>
        <th> Address</th>
        <th></th>
        

    </tr>
    @for (int i = 0; i < Model.Rows.Count; i++)
    {
<tr>
    <td>@Model.Rows[i][1]</td>
    <td>@Model.Rows[i][2]</td>
    <td>@Model.Rows[i][3]</td>
    <td>@Model.Rows[i][4]</td>
    <td>@Model.Rows[i][5]</td>
    <td>

        <a href="@Url.Action("Edit","EMPLOYEES",new {@id = Model.Rows[i][0]})">Edit</a>

    </td>

    

</tr>
    }

</table>

<a href="@Url.Action("Create","EMPLOYEES")">Add New Employee</a>

